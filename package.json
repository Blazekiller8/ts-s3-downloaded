{
  "name": "ts-s3-downloader",
  "version": "1.0.0",
  "description": "Typescript program to download all the files in a folder of an AWS S3 bucket using aws-sdk v3",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Blazekiller8/ts-s3-downloader.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "author": "Sivaraam T K",
  "license": "MIT",
  "keywords": [
    "typescript",
    "aws",
    "s3",
    "downloader"
  ],
  "scripts": {
    "test": "npm run pretest && npm run start test downloads && npm run posttest",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "npx tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "node build/src/test-upload.js",
    "posttest": "rm -rf downloads/* downloads",
    "build": "npm run clean && npm run compile",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "types": "tsc --noEmit",
    "docs": "npx typedoc --out docs ./src",
    "start": "node build/src/index.js $1 $2"
  },
  "devDependencies": {
    "eslint": "^8.49.0",
    "@types/node": "^20.6.0",
    "gts": "^3.1.1",
    "prettier": "^2.8.8",
    "typedoc": "^0.24.8",
    "typescript": "~5.1.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.359.0",
    "@elastic/ecs-winston-format": "^1.3.1",
    "@types/winston": "^2.4.4",
    "aws-sdk": "^2.1457.0",
    "dotenv": "^16.1.4",
    "fs": "^0.0.2",
    "logform": "^2.5.1",
    "stream": "^0.0.2",
    "winston-elasticsearch": "^0.17.4",
    "winston": "^3.10.0",
    "winston-transport": "^4.5.0"
  }
}