{
  "name": "ts-s3-downloader",
  "version": "1.0.0",
  "description": "Typescript program to download all the files in a folder of an AWS S3 bucket using aws-sdk v3",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Blazekiller8/ts-s3-downloader.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "author": "Sivaraam T K",
  "license": "MIT",
  "keywords": [
    "typescript",
    "aws",
    "s3",
    "downloader"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint --fix",
    "build": "npm run clean && npm run compile",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "types": "tsc --noEmit",
    "docs": "npx typedoc --out docs ./src",
    "start": "node build/src/index.js $1 $2"
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "eslint": "^8.39.0",
    "gts": "^3.1.1",
    "prettier": "^2.8.7",
    "typedoc": "^0.24.4",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.317.0",
    "dotenv": "^16.0.3",
    "fs": "^0.0.1-security",
    "stream": "^0.0.2"
  }
}